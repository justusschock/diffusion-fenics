cell = tetrahedron

scalar = FiniteElement("Lagrange", cell, 1)
vector = VectorElement("Lagrange", cell, 2)

v = TestFunction(scalar)  # Test Function
u = TrialFunction(scalar) # Trial Function
y = Coefficient(scalar)
b = Coefficient(vector)
D = Coefficient(scalar)

a = inner(u, v)*(dx(2)+dx(3)+dx(5))
L = (dot(b, grad(y)*v) + D*dot(grad(y), grad(v)))*(dx(2)+dx(3)+dx(5)) # Computes \nabla(D \nabla(y)) with velocity b